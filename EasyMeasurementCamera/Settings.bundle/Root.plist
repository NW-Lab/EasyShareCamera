				<real>20</real>
				<real>25</real>
				<real>30</real>
				<real>35</real>
				<real>40</real>
				<real>45</real>
				<real>50</real>
			</array>
		</dict>

		<dict>
			<key>Type</key>
			<string>PSMultiValueSpecifier</string>
			<key>Title</key>
