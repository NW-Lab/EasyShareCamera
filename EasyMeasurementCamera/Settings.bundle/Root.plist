		<dict>
			<key>Type</key>
			<string>PSMultiValueSpecifier</string>
			<key>Title</key>
			<string>撮影モード</string>
			<key>Key</key>
			<string>captureMode</string>
			<key>DefaultValue</key>
			<string>single</string>
